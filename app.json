{
  "expo": {
    "name": "hexaCaseStudy",
    "slug": "hexaCaseStudy",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "50.0.0",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "myapp",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#09090B"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hexa.hexacasestudy"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hexa.hexacasestudy"
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["@config-plugins/detox", "expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dfe49579-2aba-4534-9e93-afe1cebdcaca"
      }
    }
  }
}
